{
  "name": "randock/graphql-client-generator",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.2.0",
    "ext-json": "*",
    "nette/php-generator": "^3.2",
    "softonic/graphql-client": "^1.2"
  },
  "require-dev": {
    "randock/build": "dev-symfony4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Randock\\Graphql\\Generator\\": "src/Generator"
    }
  }
}
